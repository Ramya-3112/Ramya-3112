def classify_triangle(side1, side2, side3):
    if side1 == side2 == side3:
        return "equilateral"
    elif side1 == side2 or side1 == side3 or side2 == side3:
        return "isosceles"
    else:
        return "scalene"
side1 = float(input())
side2 = float(input())
side3 = float(input())
triangle_type = classify_triangle(side1, side2, side3)
print(f"That's a {triangle_type} triangle")
