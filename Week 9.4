def min_coins(target):
    denominations = [1, 2, 3, 4]
    dp = [float('inf')] * (target + 1)
    dp[0] = 0
    
    for i in range(1, target + 1):
        for coin in denominations:
            if i - coin >= 0:
                dp[i] = min(dp[i], dp[i - coin] + 1)
    
    return dp[target]

def coinChange(target):
    return min_coins(target)
