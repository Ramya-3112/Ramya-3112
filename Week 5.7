def compare_strings(string1, string2):
    string1 = string1.lower()
    string2 = string2.lower()
    
    if string1 < string2:
        return "-1"
    elif string1 > string2:
        return "1"
    else:
        return "0"

# Read input
T = int(input())
for _ in range(T):
    string1 = input()
    string2 = input()
    print(compare_strings(string1, string2))
