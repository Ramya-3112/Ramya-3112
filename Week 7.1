def main():
    import sys
    input = sys.stdin.read
    data = input().split()

    n1 = int(data[0])
    n2 = int(data[1])
    
    array1 = list(map(int, data[2:n2+2]))
    array2 = list(map(int, data[n2+2:]))

    # Convert arrays to sets to find unique elements
    set1 = set(array1)
    set2 = set(array2)
    
    # Find the unique elements by using symmetric difference
    unique_elements = list(set1.symmetric_difference(set2))
    
    # Sort the result
    unique_elements.sort()

    if unique_elements:
        # Print unique elements
        print(" ".join(map(str, unique_elements)))
        # Print the number of unique elements
        print(len(unique_elements))
    else:
        print("NO SUCH ELEMENTS")

if __name__ == "__main__":
    main()
