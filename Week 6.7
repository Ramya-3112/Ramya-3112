def find_mapping(A, B):
    mapping = {}
    for i, num in enumerate(B):
        mapping[num] = i
    return [mapping[num] for num in A]

# Read input
n = int(input().strip())  # Length of the lists
A = list(map(int, input().strip().split()))  # List A
B = list(map(int, input().strip().split()))  # List B

# Find and print the index mapping
mapping = find_mapping(A, B)
print(*mapping)
